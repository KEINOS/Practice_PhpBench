{
    "bootstrap": "vendor/autoload.php",
    "reports": {
        "time": {
            "extends": "aggregate",
            "title": "The Consumation of Time",
            "cols": [
                "subject",
                "mode",
                "mean",
                "rstdev",
                "diff"
            ],
            "sort": {
                "mean": "asc"
            }
        },
        "memory": {
            "extends": "aggregate",
            "title": "The Memory Usage",
            "cols": [
                "subject",
                "mem_real",
                "mem_final",
                "mem_peak",
                "diff"
            ],
            "diff_col": "mem_peak"
        },
        "summary": {
            "extends": "aggregate",
            "title": "The Memory Usage",
            "cols": [
                "subject",
                "mean",
                "mode",
                "mem_real",
                "mem_final",
                "mem_peak",
                "rstdev",
                "diff"
            ],
            "diff_col": "mem_peak",
            "sort": {
                "mean": "asc"
            }
        }
    }
}